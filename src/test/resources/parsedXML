Robot
	Subsystem: name[drivetrain]
		Motor: id[left] type[spark] port[0]
		Motor: id[right] type[spark] port[1]
		Encoder: id[leftEncoder] channelA[0] channelB[1]
		Encoder: id[rightEncoder] channelA[2] channelB[3]
	Subsystem: name[piston]
		DoubleSolenoid: id[dsol] portOn[4] portOff[5]
	Subsystem: name[valve]
		Solenoid: id[sol] port[3]
	Controller: id[xbox] type[xbox] port[0]

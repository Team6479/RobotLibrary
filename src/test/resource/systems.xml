<robot>
	<subsystem>
		<id>drivetrain</id>
		<components>
			<motor>
				<id>left</id>
				<port>0</port>
				<type>spark</type>
			</motor>
			<motor>
				<id>right</id>
				<port>1</port>
				<type>spark</type>
			</motor>
			<encoder>
				<id>leftEncoder</id>
				<channelA>0</channelA>
				<channelB>1</channelB>
			</encoder>
			<encoder>
				<id>rightEncoder</id>
				<channelA>2</channelA>
				<channelB>3</channelB>
			</encoder>
			<sonar>
				<id>sonar</id>
				<port>4</port>
			</sonar>
		</components>
	</subsystem>
	<subsystem>
		<id>piston</id>
		<components>
			<double_solenoid>
				<id>dsol</id>
				<portOn>4</portOn>
				<portOff>5</portOff>
			</double_solenoid>
		</components>
	</subsystem>
	<subsystem>
		<id>valve</id>
		<components>
			<solenoid>
				<id>sol</id>
				<port>3</port>
			</solenoid>
		</components>
	</subsystem>
	<controller>
		<id>xbox</id>
		<type>xbox</type>
		<port>0</port>
	</controller>
</robot>